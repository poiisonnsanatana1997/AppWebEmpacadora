# Variables de entorno de ejemplo
# Copia este archivo como .env y ajusta los valores según tu entorno

# URL base de la API
VITE_API_BASE_URL=http://localhost:5177/api

# Configuración de la aplicación
VITE_APP_NAME=AppWebEmpacadora
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development
VITE_APP_BASENAME=/empacadora

# Configuración de autenticación (opcional)
VITE_AUTH_TIMEOUT=3600000

# Configuración de logging (opcional)
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=info 